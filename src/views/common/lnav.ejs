	<script>
		// Script for open/close Modal 
		document.querySelectorAll('[data-ctModalTrigger]').forEach(single => {
			single.addEventListener('click', function (e) {
				e.preventDefault()
				const ctModal = document.querySelector(single.dataset.ctmodaltrigger)
				ctModal.classList.add('show');
				ctModal.insertAdjacentHTML('afterbegin', '<div class="ct_modal_overlay"></div>');
				ctModal.querySelector('.ct_modal_overlay').addEventListener('click', function () {
					this.closest('.ct_modal').classList.remove('show');
				})
			})
		})

		document.querySelectorAll('[data-ctModalClose]').forEach(single => {
			single.addEventListener('click', function (e) {
				e.preventDefault()
				this.closest('.ct_modal').classList.remove('show');
			})
		})


		// Script for Dropdown 
		let index = 1;

		const on = (listener, query, fn) => {
			document.querySelectorAll(query).forEach(item => {
				item.addEventListener(listener, el => {
					fn(el);
				})
			})
		}

		on('click', '.selectBtn', item => {
			const next = item.target.nextElementSibling;
			next.classList.toggle('toggle');
			next.style.zIndex = index++;
		});
		on('click', '.option', item => {
			item.target.closest('.selectDropdown').classList.remove('toggle');
			const parent = item.target.closest('.select').children[0];
			parent.setAttribute('data-type', item.target.closest('.option').getAttribute('data-type'));
			parent.innerHTML = item.target.closest('.option').querySelector('.display-text').innerHTML;
		})
	</script>
